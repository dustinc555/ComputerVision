function [S] = SSD(f1, f2)
    % f1 and f2 are EQUAL nx3 matrices
    % that are image features rotated for 
    % rotation invarience and also scale invarient as well
    
    maxI = min([size(f1, 1) size(f2, 1)]);
    
    
 
    
    S = 0;
    
    if size(f1,2) == 3 && size(f2,2) == 3 && 
        for i = 1 : maxI
            S = S + sqrt( (f2(i,1) - f1(i,1))^2 + (f2(i,2) - f1(i,2))^2 + (f2(i,3) - f1(i,3))^2 );
        end
    else
        S = intmax;
    end
    
end